{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES2020",
    "outDir": "../../dist/web",
    "lib": ["ES2020", "WebWorker", "DOM"],
    "sourceMap": true,
    "rootDir": ".",
    "strict": true,
    "types": ["webpack-env", "mocha", "vscode"],
    "moduleResolution": "node",
    "downlevelIteration": true,
    "skipLibCheck": true
  },
  "include": ["**/*"],
  "exclude": []
}
